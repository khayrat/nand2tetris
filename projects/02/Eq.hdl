/**
 * Eq gate: 
 * out = 1 if (a == b)
 *       0 otherwise
 */

CHIP Eq {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    And(a=a, b=b, out=ab);
    And(a=notA, b=notB, out=notAB);
    Or(a=ab, b=notAB, out=out);
}
