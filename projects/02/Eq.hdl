/**
  out := true if a == b
 */

CHIP Eq {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=nota, b=notb, out=notaAndnotb);
    And(a=a, b=b, out=aAndb);
    Or(a=aAndb, b=notaAndnotb, out=out);
}
